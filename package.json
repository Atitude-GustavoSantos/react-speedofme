{
  "name": "react-speedofme",
  "version": "1.0.1",
  "description": "A react component for consuming the api of speed of me",
  "repository": "https://github.com/michaeldouglas/react-speedofme",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src/index.js __tests__/index.js",
    "prepublish": "yarn run build",
    "build": "babel src --out-dir dist"
  },
  "author": "Michael Douglas",
  "keywords": ["react speedofme", "speedofme"],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "event-emitter": "^0.3.4",
    "jest": "^21.2.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-test-renderer": "^15.6.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "npm": "^5.8.0",
    "prop-types": "^15.6.0",
    "react-d3-speedometer": "^0.3.2",
    "react-display-name": "^0.2.4",
    "scriptjs": "^2.5.8"
  },
  "jest": {
    "timers": "fake",
    "automock": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      ".*mock.*",
      ".*stub.*"
    ],
    "testPathIgnorePatterns": ["<rootDir>/node_modules", ".*mock.*", ".*stub.*"]
  },
  "peerDependencies": {
    "react": ">=15"
  }
}
